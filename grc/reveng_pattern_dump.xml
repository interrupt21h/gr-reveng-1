<?xml version="1.0"?>
<block>
  <name>Pattern Dump</name>
  <key>reveng_pattern_dump</key>
  <category>reveng</category>
  <flags>deprecated</flags>
  <import>import reveng</import>
  <make>reveng.pattern_dump($pattern, $dump_len, $output_fmt, $rel_time, $file_name, $stdout)</make>

  <param>
    <name>Pattern</name>
    <key>pattern</key>
    <value>[0,1]*16</value>
    <type>raw</type>
  </param>

  <param>
    <name>Dump Length</name>
    <key>dump_len</key>
    <value>32</value>
    <type>int</type>
  </param>

  <param>
    <name>Output Format</name>
    <key>output_fmt</key>
    <value>%Y-%m-%d %H:%M:%S,%[bits]</value>
    <type>string</type>
  </param>

  <param>
    <name>Time Reference</name>
    <key>rel_time</key>
    <type>enum</type>
    <option>
        <name>Absolute</name>
        <key>False</key>
    </option>
    <option>
        <name>Relative</name>
        <key>True</key>
    </option>
    <option>
        <name>No Time</name>
        <key>None</key>
    </option>
  </param>

  <param>
    <name>File Output</name>
    <key>file_name</key>
    <value></value>
    <type>string</type>
  </param>

  <param>
    <name>Print to stdout</name>
    <key>stdout</key>
    <value>True</value>
    <type>bool</type>
    <option>
        <name>Yes</name>
        <key>True</key>
    </option>
    <option>
        <name>No</name>
        <key>False</key>
    </option>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
